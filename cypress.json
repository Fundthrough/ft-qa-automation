{
    "projectId": "ekqhck",
    "baseUrl": "https://nebula-client.fundthrough.com",
    "chromeWebSecurity": false,
    "ignoreTestFiles": "**/examples/*",
    "defaultCommandTimeout": 60000,
    "viewportWidth": 1920,
    "viewportHeight": 1080,
    "reporter": "cypress-multi-reporters",
    "reporterOptions": {
        "reporterEnabled": "mochawesome",
        "mochawesomeReporterOptions": {
            "reportDir": "cypress/reports/mocha",
            "quite": true,
            "overwrite": false,
            "html": true,
            "json": true
        }
    },
    "watchForFileChanges": false,
    "testFiles": [
        "nebula/TC01_signUp.spec.js",
        "nebula/TC02_signIn.spec.js",
        "nebula/TC03_uploadInvoice.spec.js",
        "nebula/invoicePanel.spec.js",
        "nebula/yellowCard.spec.js"
    ],
    "retries": 2
}
